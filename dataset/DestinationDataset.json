{
	"name": "DestinationDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "adls-gen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema_name": {
				"type": "string"
			},
			"table_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().schema_name, '_', dataset().table_name, formatDateTime(utcNow(), 'yyyyMMddhhmmss'), '.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('bronze/',dataset().schema_name, '/', dataset().schema_name, '_', dataset().table_name)",
					"type": "Expression"
				},
				"fileSystem": "data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.Synapse/workspaces/datasets"
}